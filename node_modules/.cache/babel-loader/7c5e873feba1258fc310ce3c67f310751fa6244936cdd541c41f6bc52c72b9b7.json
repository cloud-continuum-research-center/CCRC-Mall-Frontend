{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport { reactive } from \"vue\";\nimport Card from \"@/components/Card.vue\";\nimport router from \"@/scripts/router\";\nexport default {\n  name: \"Home\",\n  components: {\n    Card\n  },\n  setup() {\n    const state = reactive({\n      items: [],\n      searchTerm: \"\",\n      searchBy: \"\"\n    });\n    const goToItemRegistration = () => {\n      router.push({\n        path: \"/new_item\"\n      });\n    };\n    axios.get(\"/api/items/\").then(({\n      data\n    }) => {\n      state.items = data;\n    });\n    const filterItems = () => {\n      let filteredItems = state.items;\n      if (state.searchTerm.trim() !== \"\") {\n        if (state.searchBy === \"name\") {\n          filteredItems = filteredItems.filter(item => item.name.toLowerCase().includes(state.searchTerm.toLowerCase()));\n        } else if (state.searchBy === \"category\") {\n          filteredItems = filteredItems.filter(item => item.category.toLowerCase().includes(state.searchTerm.toLowerCase()));\n        }\n      }\n\n      // filteredItems에는 검색 및 필터링된 항목이 포함됩니다.\n      // 이제 state.items를 업데이트하여 화면에 보여줄 상품 목록을 갱신합니다.\n      state.items = filteredItems;\n    };\n    return {\n      state,\n      goToItemRegistration,\n      filterItems\n    };\n  }\n};","map":{"version":3,"names":["axios","reactive","Card","router","name","components","setup","state","items","searchTerm","searchBy","goToItemRegistration","push","path","get","then","data","filterItems","filteredItems","trim","filter","item","toLowerCase","includes","category"],"sources":["C:\\Users\\boo\\icns\\shop\\front-end\\src\\pages\\Home.vue"],"sourcesContent":["<template>\r\n  <div class=\"home\">\r\n    <div class=\"album py-5 bg-body-tertiary\">\r\n      <div class=\"container\">\r\n        <button @click=\"goToItemRegistration\" class=\"btn btn-primary\">상품 등록</button>\r\n        <div></div>\r\n\r\n        <!-- 검색창 -->\r\n        <input type=\"text\" v-model=\"searchTerm\" placeholder=\"검색어를 입력하세요\" />\r\n        <label><input type=\"checkbox\" v-model=\"searchBy\" value=\"name\"> 상품명으로 검색</label>\r\n        <label><input type=\"checkbox\" v-model=\"searchBy\" value=\"category\"> 카테고리로 검색</label>\r\n        <div></div>\r\n        <button @click=\"filterItems\" class=\"btn btn-primary\">검색</button>\r\n\r\n\r\n\r\n        <div class=\"row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3\">\r\n\r\n          <div class=\"col\" v-for=\"(item, idx) in state.items\" :key=\"idx\">\r\n            <Card :item=\"item\" />\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { reactive } from \"vue\";\r\nimport Card from \"@/components/Card.vue\";\r\nimport router from \"@/scripts/router\";\r\n\r\nexport default {\r\n  name: \"Home\",\r\n  components: { Card },\r\n  setup() {\r\n    const state = reactive({\r\n      items: [],\r\n      searchTerm: \"\",\r\n      searchBy: \"\"\r\n    });\r\n\r\n    const goToItemRegistration = () => {\r\n      router.push({ path: \"/new_item\" });\r\n    };\r\n\r\n    axios.get(\"/api/items/\").then(({ data }) => {\r\n      state.items = data;\r\n    });\r\n\r\n    const filterItems = () => {\r\n      let filteredItems = state.items;\r\n\r\n      if (state.searchTerm.trim() !== \"\") {\r\n        if (state.searchBy === \"name\") {\r\n          filteredItems = filteredItems.filter(item => item.name.toLowerCase().includes(state.searchTerm.toLowerCase()));\r\n        } else if (state.searchBy === \"category\") {\r\n          filteredItems = filteredItems.filter(item => item.category.toLowerCase().includes(state.searchTerm.toLowerCase()));\r\n        }\r\n      }\r\n\r\n      // filteredItems에는 검색 및 필터링된 항목이 포함됩니다.\r\n      // 이제 state.items를 업데이트하여 화면에 보여줄 상품 목록을 갱신합니다.\r\n      state.items = filteredItems;\r\n    };\r\n\r\n    return { state, goToItemRegistration,filterItems };\r\n  },\r\n\r\n\r\n};\r\n</script>\r\n\r\n<style scoped></style>"],"mappings":";AA8BA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,QAAO,QAAS,KAAK;AAC9B,OAAOC,IAAG,MAAO,uBAAuB;AACxC,OAAOC,MAAK,MAAO,kBAAkB;AAErC,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,UAAU,EAAE;IAAEH;EAAK,CAAC;EACpBI,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIN,QAAQ,CAAC;MACrBO,KAAK,EAAE,EAAE;MACTC,UAAU,EAAE,EAAE;MACdC,QAAQ,EAAE;IACZ,CAAC,CAAC;IAEF,MAAMC,oBAAmB,GAAIA,CAAA,KAAM;MACjCR,MAAM,CAACS,IAAI,CAAC;QAAEC,IAAI,EAAE;MAAY,CAAC,CAAC;IACpC,CAAC;IAEDb,KAAK,CAACc,GAAG,CAAC,aAAa,CAAC,CAACC,IAAI,CAAC,CAAC;MAAEC;IAAK,CAAC,KAAK;MAC1CT,KAAK,CAACC,KAAI,GAAIQ,IAAI;IACpB,CAAC,CAAC;IAEF,MAAMC,WAAU,GAAIA,CAAA,KAAM;MACxB,IAAIC,aAAY,GAAIX,KAAK,CAACC,KAAK;MAE/B,IAAID,KAAK,CAACE,UAAU,CAACU,IAAI,CAAC,MAAM,EAAE,EAAE;QAClC,IAAIZ,KAAK,CAACG,QAAO,KAAM,MAAM,EAAE;UAC7BQ,aAAY,GAAIA,aAAa,CAACE,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACjB,IAAI,CAACkB,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAChB,KAAK,CAACE,UAAU,CAACa,WAAW,CAAC,CAAC,CAAC,CAAC;QAChH,OAAO,IAAIf,KAAK,CAACG,QAAO,KAAM,UAAU,EAAE;UACxCQ,aAAY,GAAIA,aAAa,CAACE,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACG,QAAQ,CAACF,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAChB,KAAK,CAACE,UAAU,CAACa,WAAW,CAAC,CAAC,CAAC,CAAC;QACpH;MACF;;MAEA;MACA;MACAf,KAAK,CAACC,KAAI,GAAIU,aAAa;IAC7B,CAAC;IAED,OAAO;MAAEX,KAAK;MAAEI,oBAAoB;MAACM;IAAY,CAAC;EACpD;AAGF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}