{"ast":null,"code":"import axios from \"axios\";\nimport { reactive } from \"vue\";\nexport default {\n  props: {\n    productId: {}\n  },\n  setup(props) {\n    const state = reactive({\n      reviews: [],\n      reviewData: {\n        content: \"\",\n        star: 0,\n        user_id: 1,\n        item_id: props.productId\n      }\n    });\n    axios.get(`/api/items/${state.reviewData.item_id}/reviews/`).then(response => {\n      state.reviews = response.data;\n    });\n    const submitReview = () => {\n      const productId = props.productId;\n      console.log(productId);\n      axios.post(`/api/items/${productId}/reviews/`, state.reviewData).then(response => {\n        console.log(\"리뷰가 성공적으로 등록되었습니다:\", response.data);\n        window.alert('리뷰가 성공적으로 등록되었습니다.');\n        fetchReviews();\n      }).catch(error => {\n        console.error(\"리뷰 등록에 실패했습니다:\", error);\n      });\n    };\n    const fetchReviews = () => {\n      const productId = props.productId;\n      console.log(\"Get\", productId);\n      axios.get(`/api/items/${productId}/reviews/`).then(response => {\n        state.reviews = response.data;\n      });\n    };\n    return {\n      state,\n      submitReview,\n      fetchReviews\n    };\n  }\n};","map":{"version":3,"names":["axios","reactive","props","productId","setup","state","reviews","reviewData","content","star","user_id","item_id","get","then","response","data","submitReview","console","log","post","window","alert","fetchReviews","catch","error"],"sources":["C:\\Users\\boo\\icns\\shop\\front-end\\src\\components\\Review.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h2>리뷰{{ productId }}</h2>\r\n\r\n    <!-- 리뷰 작성 폼 -->\r\n    <form @submit.prevent=\"submitReview\">\r\n      <textarea v-model=\"state.reviewData.content\" placeholder=\"리뷰 작성란\"></textarea>\r\n      <select v-model=\"state.reviewData.star\">\r\n        <option value=\"1\">1 Star</option>\r\n        <option value=\"2\">2 Stars</option>\r\n        <option value=\"3\">3 Stars</option>\r\n        <option value=\"4\">4 Stars</option>\r\n        <option value=\"5\">5 Stars</option>\r\n      </select>\r\n      <button type=\"submit\">리뷰 등록하기</button>\r\n    </form>\r\n\r\n    <!-- 리뷰 목록 -->\r\n    <div v-if=\"state.reviews && state.reviews.length > 0\">\r\n      <h3>리뷰 목록</h3>\r\n      <ul>\r\n        <li v-for=\"(review, index) in state.reviews\" :key=\"index\">\r\n          <div>별점: {{ review.star }} Stars</div>\r\n          <div>내용: {{ review.content }}</div>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <div v-else>\r\n      <p>등록된 리뷰가 없습니다.</p>\r\n    </div>\r\n\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { reactive } from \"vue\";\r\n\r\nexport default {\r\n  props: {\r\n    productId: {\r\n    }\r\n  },\r\n  setup(props) {\r\n    const state = reactive({\r\n      reviews: [],\r\n      reviewData: {\r\n        content: \"\",\r\n        star: 0,\r\n        user_id: 1,\r\n        item_id: props.productId,\r\n      }\r\n    });\r\n\r\n      axios.get(`/api/items/${state.reviewData.item_id}/reviews/`).then(response => {\r\n        state.reviews = response.data;\r\n      });\r\n\r\n\r\n    const submitReview = () => {\r\n      const productId = props.productId;\r\n      console.log(productId);\r\n      axios.post(`/api/items/${productId}/reviews/`, state.reviewData).then((response) => {\r\n        console.log(\"리뷰가 성공적으로 등록되었습니다:\", response.data);\r\n        window.alert('리뷰가 성공적으로 등록되었습니다.');\r\n        fetchReviews();\r\n      }).catch((error) => {\r\n        console.error(\"리뷰 등록에 실패했습니다:\", error);\r\n      });\r\n    }\r\n\r\n    const fetchReviews = () => {\r\n      const productId = props.productId;\r\n      console.log(\"Get\", productId);\r\n      axios.get(`/api/items/${productId}/reviews/`).then(response => {\r\n        state.reviews = response.data;\r\n      });\r\n    }\r\n\r\n    return { state, submitReview, fetchReviews };\r\n  },\r\n\r\n\r\n};\r\n</script>"],"mappings