{"ast":null,"code":"import axios from \"axios\";\nimport { reactive } from \"vue\";\nexport default {\n  name: \"Review\",\n  props: {\n    productId: {}\n  },\n  setup(props) {\n    const state = reactive({\n      reviews: [],\n      reviewData: {\n        content: \"\",\n        star: 0,\n        user_id: 1,\n        item_id: props.productId\n      }\n    });\n    const submitReview = () => {\n      const productId = props.productId;\n      console.log(state.reviewData);\n      axios.post(`/api/items/${productId}/reviews/`, state.reviewData).then(response => {\n        console.log(\"리뷰가 성공적으로 등록되었습니다:\", response.data);\n        window.alert('리뷰가 성공적으로 등록되었습니다.');\n        fetchReviews();\n      }).catch(error => {\n        console.error(\"리뷰 등록에 실패했습니다:\", error);\n      });\n    };\n    const fetchReviews = () => {\n      const productId = props.productId;\n      console.log(\"Get\", productId);\n      axios.get(`/api/items/${productId}/reviews/`).then(response => {\n        state.reviews = response.data;\n        console.log(state.reviews);\n      });\n    };\n    return {\n      state,\n      submitReview,\n      fetchReviews\n    };\n  },\n  watch: {\n    productId() {\n      if (this.productId) {\n        this.fetchReviews();\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","reactive","name","props","productId","setup","state","reviews","reviewData","content","star","user_id","item_id","submitReview","console","log","post","then","response","data","window","alert","fetchReviews","catch","error","get","watch"],"sources":["C:\\Users\\boo\\icns\\shop\\front-end\\src\\components\\Review.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h2>리뷰{{ productId }}</h2>\r\n\r\n    <!-- 리뷰 작성 폼 -->\r\n    <form @submit.prevent=\"submitReview\">\r\n      <textarea v-model=\"state.reviewData.content\" placeholder=\"리뷰 작성란\"></textarea>\r\n      <select v-model=\"state.reviewData.star\">\r\n        <option value=\"1\">1 Star</option>\r\n        <option value=\"2\">2 Stars</option>\r\n        <option value=\"3\">3 Stars</option>\r\n        <option value=\"4\">4 Stars</option>\r\n        <option value=\"5\">5 Stars</option>\r\n      </select>\r\n      <button type=\"submit\">리뷰 등록하기</button>\r\n    </form>\r\n\r\n    <!-- 리뷰 목록 -->\r\n    <div v-if=\"state.review && state.reviews.length > 0\">\r\n      <h3>리뷰 목록</h3>\r\n      <ul>\r\n        <li v-for=\"(review, index) in state.reviews\" :key=\"index\">\r\n          <div>별점: {{ review.star }} Stars</div>\r\n          <div>내용: {{ review.content }}</div>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n    <div v-else>\r\n      <p>등록된 리뷰가 없습니다.</p>\r\n    </div>\r\n\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { reactive } from \"vue\";\r\n\r\nexport default {\r\n  name: \"Review\",\r\n  props: {\r\n    productId: {\r\n    }\r\n  },\r\n  setup(props) {\r\n    const state = reactive({\r\n      reviews: [],\r\n      reviewData: {\r\n        content: \"\",\r\n        star: 0,\r\n        user_id: 1,\r\n        item_id: props.productId,\r\n      }\r\n    });\r\n\r\n    const submitReview = () => {\r\n      const productId = props.productId;\r\n      console.log(state.reviewData);\r\n      axios.post(`/api/items/${productId}/reviews/`, state.reviewData).then((response) => {\r\n        console.log(\"리뷰가 성공적으로 등록되었습니다:\", response.data);\r\n        window.alert('리뷰가 성공적으로 등록되었습니다.');\r\n        fetchReviews();\r\n      }).catch((error) => {\r\n        console.error(\"리뷰 등록에 실패했습니다:\", error);\r\n      });\r\n    }\r\n\r\n    const fetchReviews = () => {\r\n      const productId = props.productId;\r\n      console.log(\"Get\", productId);\r\n      axios.get(`/api/items/${productId}/reviews/`).then(response => {\r\n        state.reviews = response.data;\r\n        console.log(state.reviews);\r\n      });\r\n    }\r\n\r\n\r\n    return { state, submitReview, fetchReviews };\r\n  },\r\n  watch: {\r\n    productId() {\r\n      if (this.productId) {\r\n        this.fetchReviews();\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>"],"mappings":"AAoCA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,QAAO,QAAS,KAAK;AAE9B,eAAe;EACbC,IAAI,EAAE,QAAQ;EACdC,KAAK,EAAE;IACLC,SAAS,EAAE,CACX;EACF,CAAC;EACDC,KAAKA,CAACF,KAAK,EAAE;IACX,MAAMG,KAAI,GAAIL,QAAQ,CAAC;MACrBM,OAAO,EAAE,EAAE;MACXC,UAAU,EAAE;QACVC,OAAO,EAAE,EAAE;QACXC,IAAI,EAAE,CAAC;QACPC,OAAO,EAAE,CAAC;QACVC,OAAO,EAAET,KAAK,CAACC;MACjB;IACF,CAAC,CAAC;IAEF,MAAMS,YAAW,GAAIA,CAAA,KAAM;MACzB,MAAMT,SAAQ,GAAID,KAAK,CAACC,SAAS;MACjCU,OAAO,CAACC,GAAG,CAACT,KAAK,CAACE,UAAU,CAAC;MAC7BR,KAAK,CAACgB,IAAI,CAAE,cAAaZ,SAAU,WAAU,EAAEE,KAAK,CAACE,UAAU,CAAC,CAACS,IAAI,CAAEC,QAAQ,IAAK;QAClFJ,OAAO,CAACC,GAAG,CAAC,oBAAoB,EAAEG,QAAQ,CAACC,IAAI,CAAC;QAChDC,MAAM,CAACC,KAAK,CAAC,oBAAoB,CAAC;QAClCC,YAAY,CAAC,CAAC;MAChB,CAAC,CAAC,CAACC,KAAK,CAAEC,KAAK,IAAK;QAClBV,OAAO,CAACU,KAAK,CAAC,gBAAgB,EAAEA,KAAK,CAAC;MACxC,CAAC,CAAC;IACJ;IAEA,MAAMF,YAAW,GAAIA,CAAA,KAAM;MACzB,MAAMlB,SAAQ,GAAID,KAAK,CAACC,SAAS;MACjCU,OAAO,CAACC,GAAG,CAAC,KAAK,EAAEX,SAAS,CAAC;MAC7BJ,KAAK,CAACyB,GAAG,CAAE,cAAarB,SAAU,WAAU,CAAC,CAACa,IAAI,CAACC,QAAO,IAAK;QAC7DZ,KAAK,CAACC,OAAM,GAAIW,QAAQ,CAACC,IAAI;QAC7BL,OAAO,CAACC,GAAG,CAACT,KAAK,CAACC,OAAO,CAAC;MAC5B,CAAC,CAAC;IACJ;IAGA,OAAO;MAAED,KAAK;MAAEO,YAAY;MAAES;IAAa,CAAC;EAC9C,CAAC;EACDI,KAAK,EAAE;IACLtB,SAASA,CAAA,EAAG;MACV,IAAI,IAAI,CAACA,SAAS,EAAE;QAClB,IAAI,CAACkB,YAAY,CAAC,CAAC;MACrB;IACF;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}