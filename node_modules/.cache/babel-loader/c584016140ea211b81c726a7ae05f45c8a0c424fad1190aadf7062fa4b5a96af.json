p\">\r\n                <label for=\"password\">비밀번호</label>\r\n                <input type=\"password\" id=\"password\" v-model=\"password\" />\r\n            </div>\r\n            <div class=\"form-group\">\r\n                <label for=\"passwordConfirm\">비밀번호 확인</label>\r\n                <input type=\"password\" id=\"passwordConfirm\" v-model=\"passwordConfirm\" />\r\n                <p v-if=\"password !== passwordConfirm\" style=\"color: red;\">비밀번호가 일치하지 않습니다.</p>\r\n            </div>\r\n            <button type=\"submit\">회원가입</button>\r\n        </form>\r\n    </div>\r\n</template>\r\n  \r\n<script>\r\nimport axios from 'axios';\r\nimport router from \"@/scripts/router\";\r\n\r\n\r\nexport default {\r\n    name: 'Join',\r\n    data() {\r\n        return {\r\n            email: '',\r\n            password: '',\r\n            passwordConfirm: '',\r\n           };\r\n    },\r\n    methods: {\r\n        async submitForm() {\r\n            try {\r\n                if (this.password !== this.passwordConfirm) {\r\n                    console.error('비밀번호가 일치하지 않습니다.');\r\n                    window.alert(\"비밀번호가 일치하지 않습니다\");\r\n                    return;\r\n                }\r\n                const userData = {\r\n                    email: this.email,\r\n                    password: this.password,\r\n                };\r\n\r\n                const response = await axios.post('/api/join/', userData);\r\n                //console.log('서버 응답:', response.id);\r\n                console.log('서버 응답:', response ? response.data : 'No response data');\r\n                window.alert('회원가입이 완료되었습니다.');\r\n\r\n                const moveTo = window.confirm('로그인 화면으로 이동하시겠습니까?');\r\n                console.log('moveTo:', moveTo);\r\n                if (moveTo) {\r\n                    console.log('이동: 로그인 화면');\r\n                    router.push({ name: 'Login' }); // 로그인 화면으로 이동\r\n                } else {\r\n                    console.log('이동: 메인 화면');\r\n                    router.push({ name: 'Home' }); // 메인 화면으로 이동\r\n                }\r\n\r\n            } catch (error) {\r\n                console.error('오류 발생:', error.response.data);\r\n            }\r\n        },\r\n    },\r\n};\r\n</script>\r\n  \r\n<style scoped>\r\n.join-container {\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    height: 100vh;\r\n}\r\n\r\n.join-form {\r\n    width: 300px;\r\n    padding: 20px;\r\n    border: 1px solid #ddd;\r\n    border-radius: 5px;\r\n}\r\n\r\n.form-group {\r\n    margin-bottom: 15px;\r\n}\r\n\r\nlabel {\r\n    display: block;\r\n    margin-bottom: 5px;\r\n    font-weight: bold;\r\n}\r\n\r\ninput {\r\n    width: 100%;\r\n    padding: 8px;\r\n    box-sizing: border-box;\r\n    border: 1px solid #ccc;\r\n    border-radius: 3px;\r\n}\r\n\r\nbutton {\r\n    width: 100%;\r\n    padding: 10px;\r\n    background-color: #007bff;\r\n    color: #fff;\r\n    border: none;\r\n    border-radius: 3px;\r\n    cursor: pointer;\r\n}\r\n</style>\r\n  "],"mappings":";AAsBA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,MAAK,MAAO,kBAAkB;AAGrC,eAAe;EACXC,IAAI,EAAE,MAAM;EACZC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,eAAe,EAAE;IAClB,CAAC;EACR,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,UAAUA,CAAA,EAAG;MACf,IAAI;QACA,IAAI,IAAI,CAACH,QAAO,KAAM,IAAI,CAACC,eAAe,EAAE;UACxCG,OAAO,CAACC,KAAK,CAAC,kBAAkB,CAAC;UACjCC,MAAM,CAACC,KAAK,CAAC,iBAAiB,CAAC;UAC/B;QACJ;QACA,MAAMC,QAAO,GAAI;UACbT,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACnB,CAAC;QAED,MAAMS,QAAO,GAAI,MAAMd,KAAK,CAACe,IAAI,CAAC,YAAY,EAAEF,QAAQ,CAAC;QACzD;QACAJ,OAAO,CAACO,GAAG,CAAC,QAAQ,EAAEF,QAAO,GAAIA,QAAQ,CAACX,IAAG,GAAI,kBAAkB,CAAC;QACpEQ,MAAM,CAACC,KAAK,CAAC,gBAAgB,CAAC;QAE9B,MAAMK,MAAK,GAAIN,MAAM,CAACO,OAAO,CAAC,oBAAoB,CAAC;QACnDT,OAAO,CAACO,GAAG,CAAC,SAAS,EAAEC,MAAM,CAAC;QAC9B,IAAIA,MAAM,EAAE;UACRR,OAAO,CAACO,GAAG,CAAC,YAAY,CAAC;UACzBf,MAAM,CAACkB,IAAI,CAAC;YAAEjB,IAAI,EAAE;UAAQ,CAAC,CAAC,EAAE;QACpC,OAAO;UACHO,OAAO,CAACO,GAAG,CAAC,WAAW,CAAC;U