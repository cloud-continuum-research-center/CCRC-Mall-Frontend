{"ast":null,"code":"import axios from \"axios\";\nimport { reactive, onMounted } from \"vue\";\nimport { useRoute } from 'vue-router';\nimport Review from \"@/components/Review.vue\";\nimport * as SPLAT from \"gsplat\";\nexport default {\n  name: \"ProductDetail\",\n  components: {\n    Review\n  },\n  setup() {\n    const route = useRoute();\n    const productId = route.params.id;\n    const state = reactive({\n      product: {}\n    });\n    const orderItem = () => {\n      window.alert('상품을 주문하였습니다!');\n    };\n    const canvas = document.getElementById(\"canvas\");\n    const scene = new SPLAT.Scene();\n    const camera = new SPLAT.Camera();\n    const renderer = new SPLAT.WebGLRenderer();\n    const controls = new SPLAT.OrbitControls(camera, canvas);\n    async function main() {\n      const url = \"https://huggingface.co/datasets/dylanebert/3dgs/resolve/main/bonsai/bonsai-7k.splat\";\n      await SPLAT.Loader.LoadAsync(url, scene, () => {});\n      const frame = () => {\n        controls.update();\n        renderer.render(scene, camera);\n        requestAnimationFrame(frame);\n      };\n      requestAnimationFrame(frame);\n    }\n    main();\n    onMounted(() => {\n      axios.get(`/api/items/${productId}`).then(({\n        data\n      }) => {\n        state.product = data;\n      }).catch(error => {\n        console.error(\"Error fetching product:\", error);\n      });\n    });\n    return {\n      state,\n      orderItem\n    };\n  }\n};","map":{"version":3,"names":["axios","reactive","onMounted","useRoute","Review","SPLAT","name","components","setup","route","productId","params","id","state","product","orderItem","window","alert","canvas","document","getElementById","scene","Scene","camera","Camera","renderer","WebGLRenderer","controls","OrbitControls","main","url","Loader","LoadAsync","frame","update","render","requestAnimationFrame","get","then","data","catch","error","console"],"sources":["C:\\Users\\boo\\icns\\shop\\front-end\\src\\pages\\ProductDetail.vue"],"sourcesContent":["<template>\r\n  <div class=\"product-detail\">\r\n    <!-- 상품 상세 정보 -->\r\n    <div v-if=\"state.product\">\r\n      <h2>{{ state.product.name }}</h2>\r\n      <p>Description: {{ state.product.description }}</p>\r\n      <p>Price: {{ state.product.price }}</p>\r\n    </div>\r\n    <div v-else>\r\n      <p>Loading...</p>\r\n    </div>\r\n    <button @click=\"orderItem\" class=\"btn btn-primary\">주문하기</button>\r\n\r\n    <!-- 3D 모델을 표시할 캔버스 -->\r\n    <canvas ref=\"canvas\"></canvas>\r\n\r\n    <Review :productId=\"state.product.id\" />\r\n  </div>\r\n\r\n\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { reactive, onMounted } from \"vue\";\r\nimport { useRoute } from 'vue-router';\r\nimport Review from \"@/components/Review.vue\";\r\nimport * as SPLAT from \"gsplat\";\r\n\r\nexport default {\r\n  name: \"ProductDetail\",\r\n  components: { Review },\r\n\r\n  setup() {\r\n    const route = useRoute();\r\n    const productId = route.params.id;\r\n\r\n    const state = reactive({\r\n      product: {},\r\n    });\r\n\r\n    const orderItem = () => {\r\n      window.alert('상품을 주문하였습니다!');\r\n    };\r\n    const canvas = document.getElementById(\"canvas\");\r\n    const scene = new SPLAT.Scene();\r\n    const camera = new SPLAT.Camera();\r\n    const renderer = new SPLAT.WebGLRenderer();\r\n    const controls = new SPLAT.OrbitControls(camera, canvas);\r\n    async function main() {\r\n    const url = \"https://huggingface.co/datasets/dylanebert/3dgs/resolve/main/bonsai/bonsai-7k.splat\";\r\n\r\n    await SPLAT.Loader.LoadAsync(url, scene, () => {});\r\n\r\n    const frame = () => {\r\n        controls.update();\r\n        renderer.render(scene, camera);\r\n\r\n        requestAnimationFrame(frame);\r\n    };\r\n\r\n    requestAnimationFrame(frame);\r\n}\r\n\r\nmain();\r\n\r\n    onMounted(() => {\r\n      axios.get(`/api/items/${productId}`)\r\n        .then(({ data }) => {\r\n          state.product = data;\r\n        })\r\n        .catch(error => {\r\n          console.error(\"Error fetching product:\", error);\r\n        });\r\n    });\r\n\r\n\r\n    return { state, orderItem };\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAuBA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,QAAQ,EAAEC,SAAQ,QAAS,KAAK;AACzC,SAASC,QAAO,QAAS,YAAY;AACrC,OAAOC,MAAK,MAAO,yBAAyB;AAC5C,OAAO,KAAKC,KAAI,MAAO,QAAQ;AAE/B,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IAAEH;EAAO,CAAC;EAEtBI,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIN,QAAQ,CAAC,CAAC;IACxB,MAAMO,SAAQ,GAAID,KAAK,CAACE,MAAM,CAACC,EAAE;IAEjC,MAAMC,KAAI,GAAIZ,QAAQ,CAAC;MACrBa,OAAO,EAAE,CAAC;IACZ,CAAC,CAAC;IAEF,MAAMC,SAAQ,GAAIA,CAAA,KAAM;MACtBC,MAAM,CAACC,KAAK,CAAC,cAAc,CAAC;IAC9B,CAAC;IACD,MAAMC,MAAK,GAAIC,QAAQ,CAACC,cAAc,CAAC,QAAQ,CAAC;IAChD,MAAMC,KAAI,GAAI,IAAIhB,KAAK,CAACiB,KAAK,CAAC,CAAC;IAC/B,MAAMC,MAAK,GAAI,IAAIlB,KAAK,CAACmB,MAAM,CAAC,CAAC;IACjC,MAAMC,QAAO,GAAI,IAAIpB,KAAK,CAACqB,aAAa,CAAC,CAAC;IAC1C,MAAMC,QAAO,GAAI,IAAItB,KAAK,CAACuB,aAAa,CAACL,MAAM,EAAEL,MAAM,CAAC;IACxD,eAAeW,IAAIA,CAAA,EAAG;MACtB,MAAMC,GAAE,GAAI,qFAAqF;MAEjG,MAAMzB,KAAK,CAAC0B,MAAM,CAACC,SAAS,CAACF,GAAG,EAAET,KAAK,EAAE,MAAM,CAAC,CAAC,CAAC;MAElD,MAAMY,KAAI,GAAIA,CAAA,KAAM;QAChBN,QAAQ,CAACO,MAAM,CAAC,CAAC;QACjBT,QAAQ,CAACU,MAAM,CAACd,KAAK,EAAEE,MAAM,CAAC;QAE9Ba,qBAAqB,CAACH,KAAK,CAAC;MAChC,CAAC;MAEDG,qBAAqB,CAACH,KAAK,CAAC;IAChC;IAEAJ,IAAI,CAAC,CAAC;IAEF3B,SAAS,CAAC,MAAM;MACdF,KAAK,CAACqC,GAAG,CAAE,cAAa3B,SAAU,EAAC,EAChC4B,IAAI,CAAC,CAAC;QAAEC;MAAK,CAAC,KAAK;QAClB1B,KAAK,CAACC,OAAM,GAAIyB,IAAI;MACtB,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,yBAAyB,EAAEA,KAAK,CAAC;MACjD,CAAC,CAAC;IACN,CAAC,CAAC;IAGF,OAAO;MAAE5B,KAAK;MAAEE;IAAU,CAAC;EAC7B;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}