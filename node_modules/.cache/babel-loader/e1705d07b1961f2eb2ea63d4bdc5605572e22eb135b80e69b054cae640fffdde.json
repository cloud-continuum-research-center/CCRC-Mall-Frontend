{"ast":null,"code":"import axios from \"axios\";\nimport { reactive, onMounted } from \"vue\";\nimport { useRoute } from 'vue-router';\nimport Review from \"@/components/Review.vue\";\nimport * as SPLAT from \"https://cdn.jsdelivr.net/npm/gsplat@latest\";\nconst canvas = document.getElementById(\"canvas\");\nconst progressDialog = document.getElementById(\"progress-dialog\");\nconst progressIndicator = document.getElementById(\"progress-indicator\");\nconst renderer = new SPLAT.WebGLRenderer(canvas);\nconst scene = new SPLAT.Scene();\nconst camera = new SPLAT.Camera();\nconst controls = new SPLAT.OrbitControls(camera, canvas);\nasync function main() {\n  const url = \"https://huggingface.co/datasets/dylanebert/3dgs/resolve/main/bonsai/bonsai-7k-mini.splat\";\n  await SPLAT.Loader.LoadAsync(url, scene, progress => progressIndicator.value = progress * 100);\n  progressDialog.close();\n  const handleResize = () => {\n    renderer.setSize(window.innerWidth, window.innerHeight);\n  };\n  const frame = () => {\n    controls.update();\n    renderer.render(scene, camera);\n    requestAnimationFrame(frame);\n  };\n  handleResize();\n  window.addEventListener(\"resize\", handleResize);\n  requestAnimationFrame(frame);\n}\nmain();\nexport default {\n  name: \"ProductDetail\",\n  components: {\n    Review\n  },\n  setup() {\n    const route = useRoute();\n    const productId = route.params.id;\n    const state = reactive({\n      product: {}\n    });\n    const orderItem = () => {\n      window.alert('상품을 주문하였습니다!');\n    };\n    onMounted(() => {\n      axios.get(`/api/items/${productId}`).then(({\n        data\n      }) => {\n        state.product = data;\n      }).catch(error => {\n        console.error(\"Error fetching product:\", error);\n      });\n    });\n    return {\n      state,\n      orderItem\n    };\n  }\n};","map":{"version":3,"names":["axios","reactive","onMounted","useRoute","Review","SPLAT","canvas","document","getElementById","progressDialog","progressIndicator","renderer","WebGLRenderer","scene","Scene","camera","Camera","controls","OrbitControls","main","url","Loader","LoadAsync","progress","value","close","handleResize","setSize","window","innerWidth","innerHeight","frame","update","render","requestAnimationFrame","addEventListener","name","components","setup","route","productId","params","id","state","product","orderItem","alert","get","then","data","catch","error","console"],"sources":["C:\\Users\\boo\\icns\\shop\\front-end\\src\\pages\\ProductDetail.vue"],"sourcesContent":["<template>\r\n  <div class=\"product-detail\">\r\n    <!-- 상품 상세 정보 -->\r\n    <div v-if=\"state.product\">\r\n      <h2>{{ state.product.name }}</h2>\r\n      <p>Description: {{ state.product.description }}</p>\r\n      <p>Price: {{ state.product.price }}</p>\r\n    </div>\r\n    <div v-else>\r\n      <p>Loading...</p>\r\n    </div>\r\n    <button @click=\"orderItem\" class=\"btn btn-primary\">주문하기</button>\r\n\r\n    <Review :productId=\"state.product.id\"/>\r\n  </div>\r\n\r\n\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { reactive, onMounted } from \"vue\";\r\nimport { useRoute } from 'vue-router';\r\nimport Review from \"@/components/Review.vue\";\r\n\r\nimport * as SPLAT from \"https://cdn.jsdelivr.net/npm/gsplat@latest\";\r\nconst canvas = document.getElementById(\"canvas\");\r\nconst progressDialog = document.getElementById(\"progress-dialog\");\r\nconst progressIndicator = document.getElementById(\"progress-indicator\");\r\nconst renderer = new SPLAT.WebGLRenderer(canvas);\r\nconst scene = new SPLAT.Scene();\r\nconst camera = new SPLAT.Camera();\r\nconst controls = new SPLAT.OrbitControls(camera, canvas);\r\nasync function main() {\r\n    const url = \"https://huggingface.co/datasets/dylanebert/3dgs/resolve/main/bonsai/bonsai-7k-mini.splat\";\r\n    await SPLAT.Loader.LoadAsync(url, scene, (progress) => progressIndicator.value = progress * 100);\r\n    progressDialog.close();\r\n    const handleResize = () => {\r\n        renderer.setSize(window.innerWidth, window.innerHeight);\r\n    };\r\n    const frame = () => {\r\n        controls.update();\r\n        renderer.render(scene, camera);\r\n        requestAnimationFrame(frame);\r\n    };\r\n    handleResize();\r\n    window.addEventListener(\"resize\", handleResize);\r\n    requestAnimationFrame(frame);\r\n}\r\nmain();\r\n\r\n\r\n\r\nexport default {\r\n  name: \"ProductDetail\",\r\n  components: { Review },\r\n\r\n  setup() {\r\n    const route = useRoute();\r\n    const productId = route.params.id;\r\n\r\n    const state = reactive({\r\n      product: {},\r\n    });\r\n    \r\n    const orderItem = () => {\r\n      window.alert('상품을 주문하였습니다!');\r\n    };\r\n\r\n\r\n    onMounted(() => {\r\n      axios.get(`/api/items/${productId}`)\r\n        .then(({ data }) => {\r\n          state.product = data;\r\n        })\r\n        .catch(error => {\r\n          console.error(\"Error fetching product:\", error);\r\n        });\r\n    });\r\n\r\n    return { state, orderItem };\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAoBA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,QAAQ,EAAEC,SAAQ,QAAS,KAAK;AACzC,SAASC,QAAO,QAAS,YAAY;AACrC,OAAOC,MAAK,MAAO,yBAAyB;AAE5C,OAAO,KAAKC,KAAI,MAAO,4CAA4C;AACnE,MAAMC,MAAK,GAAIC,QAAQ,CAACC,cAAc,CAAC,QAAQ,CAAC;AAChD,MAAMC,cAAa,GAAIF,QAAQ,CAACC,cAAc,CAAC,iBAAiB,CAAC;AACjE,MAAME,iBAAgB,GAAIH,QAAQ,CAACC,cAAc,CAAC,oBAAoB,CAAC;AACvE,MAAMG,QAAO,GAAI,IAAIN,KAAK,CAACO,aAAa,CAACN,MAAM,CAAC;AAChD,MAAMO,KAAI,GAAI,IAAIR,KAAK,CAACS,KAAK,CAAC,CAAC;AAC/B,MAAMC,MAAK,GAAI,IAAIV,KAAK,CAACW,MAAM,CAAC,CAAC;AACjC,MAAMC,QAAO,GAAI,IAAIZ,KAAK,CAACa,aAAa,CAACH,MAAM,EAAET,MAAM,CAAC;AACxD,eAAea,IAAIA,CAAA,EAAG;EAClB,MAAMC,GAAE,GAAI,0FAA0F;EACtG,MAAMf,KAAK,CAACgB,MAAM,CAACC,SAAS,CAACF,GAAG,EAAEP,KAAK,EAAGU,QAAQ,IAAKb,iBAAiB,CAACc,KAAI,GAAID,QAAO,GAAI,GAAG,CAAC;EAChGd,cAAc,CAACgB,KAAK,CAAC,CAAC;EACtB,MAAMC,YAAW,GAAIA,CAAA,KAAM;IACvBf,QAAQ,CAACgB,OAAO,CAACC,MAAM,CAACC,UAAU,EAAED,MAAM,CAACE,WAAW,CAAC;EAC3D,CAAC;EACD,MAAMC,KAAI,GAAIA,CAAA,K