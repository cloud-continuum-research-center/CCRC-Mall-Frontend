{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport { reactive } from \"vue\";\nimport Card from \"@/components/Card.vue\";\nimport router from \"@/scripts/router\";\nexport default {\n  name: \"Home\",\n  components: {\n    Card\n  },\n  setup() {\n    const state = reactive({\n      items: [],\n      searchTerm: \"책상1\",\n      searchBy: \"name\"\n    });\n    axios.get(\"/api/items/\").then(({\n      data\n    }) => {\n      state.items = data;\n    });\n    const goToItemRegistration = () => {\n      router.push({\n        path: \"/new_item\"\n      });\n    };\n    const filterItems = () => {\n      let filteredItems = state.items;\n      console.log(\"filterItems\");\n      console.log(state.searchTerm);\n      if (state.searchTerm.trim() !== \"\") {\n        console.log(\"if\");\n        console.log(state.searchBy);\n        if (state.searchBy === \"name\") {\n          console.log(\"searchBy name\");\n          filteredItems = filteredItems.filter(item => item.name.toLowerCase().includes(state.searchTerm.toLowerCase()));\n        } else if (state.searchBy === \"category\") {\n          console.log(\"searchBy category\");\n          filteredItems = filteredItems.filter(item => item.category.toLowerCase().includes(state.searchTerm.toLowerCase()));\n        }\n      }\n      state.items = filteredItems;\n    };\n    return {\n      state,\n      goToItemRegistration,\n      filterItems\n    };\n  }\n};","map":{"version":3,"names":["axios","reactive","Card","router","name","components","setup","state","items","searchTerm","searchBy","get","then","data","goToItemRegistration","push","path","filterItems","filteredItems","console","log","trim","filter","item","toLowerCase","includes","category"],"sources":["C:\\Users\\boo\\icns\\shop\\front-end\\src\\pages\\Home.vue"],"sourcesContent":["<template>\r\n  <div class=\"home\">\r\n    <div class=\"album py-5 bg-body-tertiary\">\r\n      <div class=\"container\">\r\n        <button @click=\"goToItemRegistration\" class=\"btn btn-primary\">상품 등록</button>\r\n        <div></div>\r\n\r\n        <!-- 검색창 -->\r\n        <input type=\"text\" v-model=\"state.searchTerm\" placeholder=\"검색어를 입력하세요\" />\r\n        <label><input type=\"checkbox\" v-model=\"searchBy\" value=\"name\"> 상품명으로 검색</label>\r\n        <label><input type=\"checkbox\" v-model=\"searchBy\" value=\"category\"> 카테고리로 검색</label>\r\n        <div></div>\r\n        <button @click=\"filterItems\" class=\"btn btn-primary\">검색</button>\r\n\r\n\r\n\r\n        <div class=\"row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3\">\r\n\r\n          <div class=\"col\" v-for=\"(item, idx) in state.items\" :key=\"idx\">\r\n            <Card :item=\"item\" />\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { reactive } from \"vue\";\r\nimport Card from \"@/components/Card.vue\";\r\nimport router from \"@/scripts/router\";\r\n\r\nexport default {\r\n\r\n  name: \"Home\",\r\n  components: { Card },\r\n\r\n  setup() {\r\n    const state = reactive({\r\n      items: [],\r\n      searchTerm: \"책상1\",\r\n      searchBy: \"name\"\r\n    });\r\n\r\n    axios.get(\"/api/items/\").then(({ data }) => {\r\n      state.items = data;\r\n    });\r\n\r\n    const goToItemRegistration = () => {\r\n      router.push({ path: \"/new_item\" });\r\n    };\r\n\r\n\r\n    const filterItems = () => {\r\n      let filteredItems = state.items;\r\n      console.log(\"filterItems\");\r\n      console.log(state.searchTerm);\r\n      if (state.searchTerm.trim() !== \"\") {\r\n        console.log(\"if\");\r\n        console.log(state.searchBy);\r\n        if (state.searchBy === \"name\") {\r\n          console.log(\"searchBy name\");\r\n          filteredItems = filteredItems.filter(item => item.name.toLowerCase().includes(state.searchTerm.toLowerCase()));\r\n        } else if (state.searchBy === \"category\") {\r\n          console.log(\"searchBy category\");\r\n          filteredItems = filteredItems.filter(item => item.category.toLowerCase().includes(state.searchTerm.toLowerCase()));\r\n\r\n        }\r\n      }\r\n      state.items = filteredItems;\r\n    };\r\n    return { state, goToItemRegistration, filterItems };\r\n  },\r\n\r\n\r\n};\r\n</script>\r\n\r\n<style scoped></style>"],"mappings":";AA8BA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,QAAO,QAAS,KAAK;AAC9B,OAAOC,IAAG,MAAO,uBAAuB;AACxC,OAAOC,MAAK,MAAO,kBAAkB;AAErC,eAAe;EAEbC,IAAI,EAAE,MAAM;EACZC,UAAU,EAAE;IAAEH;EAAK,CAAC;EAEpBI,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIN,QAAQ,CAAC;MACrBO,KAAK,EAAE,EAAE;MACTC,UAAU,EAAE,KAAK;MACjBC,QAAQ,EAAE;IACZ,CAAC,CAAC;IAEFV,KAAK,CAACW,GAAG,CAAC,aAAa,CAAC,CAACC,IAAI,CAAC,CAAC;MAAEC;IAAK,CAAC,KAAK;MAC1CN,KAAK,CAACC,KAAI,GAAIK,IAAI;IACpB,CAAC,CAAC;IAEF,MAAMC,oBAAmB,GAAIA,CAAA,KAAM;MACjCX,MAAM,CAACY,IAAI,CAAC;QAAEC,IAAI,EAAE;MAAY,CAAC,CAAC;IACpC,CAAC;IAGD,MAAMC,WAAU,GAAIA,CAAA,KAAM;MACxB,IAAIC,aAAY,GAAIX,KAAK,CAACC,KAAK;MAC/BW,OAAO,CAACC,GAAG,CAAC,aAAa,CAAC;MAC1BD,OAAO,CAACC,GAAG,CAACb,KAAK,CAACE,UAAU,CAAC;MAC7B,IAAIF,KAAK,CAACE,UAAU,CAACY,IAAI,CAAC,MAAM,EAAE,EAAE;QAClCF,OAAO,CAACC,GAAG,CAAC,IAAI,CAAC;QACjBD,OAAO,CAACC,GAAG,CAACb,KAAK,CAACG,QAAQ,CAAC;QAC3B,IAAIH,KAAK,CAACG,QAAO,KAAM,MAAM,EAAE;UAC7BS,OAAO,CAACC,GAAG,CAAC,eAAe,CAAC;UAC5BF,aAAY,GAAIA,aAAa,CAACI,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACnB,IAAI,CAACoB,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAClB,KAAK,CAACE,UAAU,CAACe,WAAW,CAAC,CAAC,CAAC,CAAC;QAChH,OAAO,IAAIjB,KAAK,CAACG,QAAO,KAAM,UAAU,EAAE;UACxCS,OAAO,CAACC,GAAG,CAAC,mBAAmB,CAAC;UAChCF,aAAY,GAAIA,aAAa,CAACI,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACG,QAAQ,CAACF,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAClB,KAAK,CAACE,UAAU,CAACe,WAAW,CAAC,CAAC,CAAC,CAAC;QAEpH;MACF;MACAjB,KAAK,CAACC,KAAI,GAAIU,aAAa;IAC7B,CAAC;IACD,OAAO;MAAEX,KAAK;MAAEO,oBAAoB;MAAEG;IAAY,CAAC;EACrD;AAGF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}