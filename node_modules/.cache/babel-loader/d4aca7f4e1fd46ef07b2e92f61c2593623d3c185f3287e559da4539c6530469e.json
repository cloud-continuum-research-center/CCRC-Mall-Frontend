ion = () => {\r\n      router.push({ path: \"/new_item\" });\r\n    };\r\n\r\n\r\n    const filterItems = () => {\r\n      let filteredItems = state.items;\r\n      console.log(\"filterItems\");\r\n      console.log(state.searchTerm);\r\n      if (state.searchTerm.trim() !== \"\") {\r\n        console.log(\"if\");\r\n        console.log(state.searchByName);\r\n        console.log(state.searchByCategory);\r\n        if (state.searchByName) {\r\n          console.log(\"searchBy name\");\r\n          filteredItems = filteredItems.filter(item => item.name.toLowerCase().includes(state.searchTerm.toLowerCase()));\r\n        } else if (state.searchByCategory) {\r\n\r\n          const selectedCategory = categories.find(category => category.name.toLowerCase().includes(state.searchTerm.toLowerCase()));\r\n          if (selectedCategory) {\r\n            filteredItems = filteredItems.filter(item => item.category_id === selectedCategory.id);\r\n          }\r\n        }\r\n      }\r\n      state.items = filteredItems;\r\n    };\r\n    return { state,fetchItems, goToItemRegistration, filterItems };\r\n  },\r\n\r\n\r\n};\r\n</script>\r\n\r\n<style scoped></style>"],"mappings":";AA8BA,SAASA,UAAS,QAAS,sBAAsB;AACjD,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,QAAO,QAAS,KAAK;AAC9B,OAAOC,IAAG,MAAO,uBAAuB;AACxC,OAAOC,MAAK,MAAO,kBAAkB;AAGrC,eAAe;EAEbC,IAAI,EAAE,MAAM;EACZC,UAAU,EAAE;IAAEH;EAAK,CAAC;EAEpBI,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIN,QAAQ,CAAC;MACrBO,KAAK,EAAE,EAAE;MACTC,UAAU,EAAE,EAAE;MACdC,YAAY,EAAE,EAAE;MAChBC,gBAAgB,EAAE;IACpB,CAAC,CAAC;IAEF,MAAMC,UAAS,GAAIA,CAAA,KAAM;MACvBZ,KAAK,CAACa,GAAG,CAAC,aAAa,CAAC,CAACC,IAAI,CAAC,CAAC;QAAEC;MAAK,CAAC,KAAK;QAC1CR,KAAK,CAACC,KAAI,GAAIO,IAAI;MACpB,CAAC,CAAC;IACJ;IAEAH,UAAU;IAEV,MAAMI,oBAAmB,GAAIA,CAAA,KAAM;MACjCb,MAAM,CAACc,IAAI,CAAC;QAAEC,IAAI,EAAE;MAAY,CAAC,CAAC;IACpC,CAAC;IAGD,MAAMC,WAAU,GAAIA,CAAA,KAAM;MACxB,IAAIC,aAAY,GAAIb,KAAK,CAACC,KAAK;MAC/Ba,OAAO,CAACC,GAAG,CAAC,aAAa,CAAC;MAC1BD,OAAO,CAACC,GAAG,CAACf,KAAK,CAACE,UAAU,CAAC;MAC7B,IAAIF,KAAK,C