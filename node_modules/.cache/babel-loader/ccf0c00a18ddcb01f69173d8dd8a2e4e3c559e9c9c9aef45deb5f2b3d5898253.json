onst imgSrc = `/api/items/${productId}/image`;\r\n      itemImg.value = imgSrc;\r\n\r\n    });\r\n\r\n    return { state, itemImg, orderItem, isOrderModalVisible, placeOrder, closeOrderModal, modelUrl, openPopup, closePopup, isPopupVisible };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.text44{\r\n  text-align: center; color: black; font-size: 16px; font-family: Poppins; font-weight: 400; word-wrap: break-word;\r\n}\r\n\r\n.star-1,.star-2,.star-3,.star-4,.star-5{\r\n  width: 20px; height: 19.15px; background: #e4e3e3;\r\n}\r\n\r\n.star-container{\r\n  width: 24px; height: 24px; padding-top: 2.35px; padding-bottom: 2.50px; padding-left: 2px; padding-right: 2px; justify-content: center; align-items: center; display: inline-flex;\r\n}\r\n\r\n.star{\r\n  width: 120px; height: 24px; justify-content: flex-start; align-items: flex-start; display: inline-flex;\r\n}\r\n\r\n.rating{\r\n  width: 152px; height: 24px; justify-content: flex-start; align-items: flex-start; gap: 8px; display: inline-flex;\r\n}\r\n\r\n.frame14{\r\n  width: 152px; height: 53px; flex-direction: column; justify-content: flex-start; align-items: flex-start; gap: 8px; display: inline-flex;\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n.product-detail {\r\n  max-width: 800px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n}\r\n\r\n.description,\r\n.price {\r\n  margin-bottom: 10px;\r\n}\r\n\r\n\r\n.modal-overlay {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: rgba(0, 0, 0, 0.5);\r\n  /* 반투명한 배경 */\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  z-index: 9999;\r\n}\r\n\r\n.modal-content {\r\n  background-color: #fff;\r\n  padding: 20px;\r\n  border-radius: 10px;\r\n  box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.3);\r\n  width: 300px;\r\n  /* 너비 조정 */\r\n  max-width: 80%;\r\n  /* 최대 너비 설정 */\r\n  max-height: 80%;\r\n  /* 최대 높이 설정 */\r\n  overflow: auto;\r\n  /* 내용이 넘칠 경우 스크롤 표시 */\r\n}\r\n\r\n.popup-overlay {\r\n  position: f