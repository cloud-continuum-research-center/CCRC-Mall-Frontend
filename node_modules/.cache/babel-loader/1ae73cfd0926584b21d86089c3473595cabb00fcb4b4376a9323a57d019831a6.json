div class=\"original-price\">Rp 2.500.000</div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport router from \"@/scripts/router\";\r\nimport { categories } from \"@/scripts/categories\";\r\n\r\nexport default {\r\n  name: \"CustomCard\",\r\n  props: {\r\n    item: Object\r\n  },\r\n  setup(props) {\r\n    const imagePath=getImagePath(props.item.image);\r\n    const imageTEST=TEST();\r\n\r\n    console.log(imagePath);\r\n\r\n    const goToProductDetail = () => {\r\n      router.push({ name: 'ProductDetail', params: { id: props.item.id } });\r\n    };\r\n    const getCategoryName = (categoryId) => {\r\n      const category = categories.find(cat => cat.id === categoryId);\r\n      return category ? category.name : \"Unknown Category\";\r\n    };\r\n\r\n    function getImagePath(image){\r\n      return `../../../backend/photo/${image.split('\\\\').pop()}`;\r\n      }\r\n    function TEST(){\r\n      return `/logo.png`;\r\n    }\r\n\r\n    return { goToProductDetail, getCategoryName, imagePath,imageTEST };\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.custom-card {\r\n  width: 285px;\r\n  height: 446px;\r\n  position: relative;\r\n}\r\n\r\n.image-container {\r\n  width: 100%;\r\n  height: 301px;\r\n  position: absolute;\r\n}\r\n\r\n.image-container img {\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n\r\n.content-container {\r\n  width: 100%;\r\n  height: 145px;\r\n  position: absolute;\r\n  bottom: 0;\r\n  background: #F4F5F7;\r\n  padding: 16px;\r\n}\r\n\r\n.product-category-container {\r\n  width: 125px;\r\n  height: 61px;\r\n  position: relative;\r\n}\r\n\r\n.product-name {\r\n  color: #3A3A3A;\r\n  font-size: 24px;\r\n  font-family: Gilroy;\r\n  font-weight: 600;\r\n  line-height: 28.80px;\r\n  word-wrap: break-word;\r\n}\r\n\r\n.category {\r\n  color: #898989;\r\n  font-size: 16px;\r\n  font-family: Gilroy;\r\n  font-weight: 500;\r\n  line-height: 24px;\r\n  word-wrap: break-word;\r\n}\r\n\r\n\r\n.price {\r\n  width: 238px;\r\n 