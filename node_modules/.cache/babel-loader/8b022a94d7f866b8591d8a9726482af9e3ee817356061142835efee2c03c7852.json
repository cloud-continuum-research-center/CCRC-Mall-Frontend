{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport router from \"@/scripts/router\";\nimport { categories } from \"@/scripts/categories\";\nimport { ref, onMounted } from \"vue\";\nexport default {\n  name: \"SimilarCustomCard\",\n  props: {\n    item: Object\n  },\n  setup(props) {\n    const itemImg = ref(null);\n    const goToProductDetail = () => {\n      window.scrollTo({\n        top: 0,\n        behavior: 'smooth'\n      });\n      window.addEventListener('scroll', handleScrollComplete);\n    };\n    const handleScrollComplete = () => {\n      if (window.innerHeight + window.scrollY >= document.body.offsetHeight) {\n        // 페이지의 끝까지 스크롤을 올렸을 때 새로고침 실행\n        window.removeEventListener('scroll', handleScrollComplete);\n        router.push({\n          name: 'ProductDetail',\n          params: {\n            id: props.item.id\n          }\n        });\n        window.location.reload();\n      }\n    };\n    const getCategoryName = categoryId => {\n      const category = categories.find(cat => cat.id === categoryId);\n      return category ? category.name : \"Unknown Category\";\n    };\n    const formatPrice = price => {\n      if (!price) return '';\n      return price.toLocaleString(\"ko-KR\") + \"원\";\n    };\n    const discountPrice = price => {\n      if (!price) return '';\n      return (price * 0.75).toLocaleString(\"ko-KR\") + \"원\";\n    };\n    onMounted(() => {\n      const imgSrc = `/api/items/${props.item.id}/image`;\n      itemImg.value = imgSrc;\n    });\n    return {\n      goToProductDetail,\n      getCategoryName,\n      itemImg,\n      formatPrice,\n      discountPrice\n    };\n  }\n};","map":{"version":3,"names":["router","categories","ref","onMounted","name","props","item","Object","setup","itemImg","goToProductDetail","window","scrollTo","top","behavior","addEventListener","handleScrollComplete","innerHeight","scrollY","document","body","offsetHeight","removeEventListener","push","params","id","location","reload","getCategoryName","categoryId","category","find","cat","formatPrice","price","toLocaleString","discountPrice","imgSrc","value"],"sources":["C:\\Users\\boo\\icns\\shop\\front-end\\src\\components\\SimilarCard.vue"],"sourcesContent":["<template>\r\n  <div class=\"similar-custom-card\" @click=\"goToProductDetail\">\r\n\r\n\r\n    <div class=\"image-container\">\r\n      <img :src=\"itemImg\" alt=\"Product Image\">\r\n    </div>\r\n\r\n    <div class=\"content-container\">\r\n      <div class=\"product-category-container\">\r\n        <h5 class=\"product-name\">{{ item.name }}</h5>\r\n        <p class=\"category\">카테고리 : {{ getCategoryName(item.category_id) }}</p>\r\n      </div>\r\n      <div class=\"price\">\r\n        <div class=\"discounted-price\">{{ discountPrice(item.price) }}</div>\r\n        <div class=\"original-price\">{{ formatPrice(item.price) }}</div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport router from \"@/scripts/router\";\r\nimport { categories } from \"@/scripts/categories\";\r\nimport { ref, onMounted } from \"vue\";\r\n\r\n\r\nexport default {\r\n  name: \"SimilarCustomCard\",\r\n  props: {\r\n    item: Object\r\n  },\r\n  setup(props) {\r\n    const itemImg = ref(null);\r\n\r\n    const goToProductDetail = () => {\r\n\r\n      window.scrollTo({ top: 0, behavior: 'smooth' });\r\n      window.addEventListener('scroll', handleScrollComplete);\r\n\r\n\r\n    };\r\n\r\n\r\n    const handleScrollComplete = () => {\r\n      if ((window.innerHeight + window.scrollY) >= document.body.offsetHeight) {\r\n        // 페이지의 끝까지 스크롤을 올렸을 때 새로고침 실행\r\n        window.removeEventListener('scroll', handleScrollComplete);\r\n        router.push({ name: 'ProductDetail', params: { id: props.item.id } });\r\n        window.location.reload();\r\n      }\r\n    };\r\n\r\n    const getCategoryName = (categoryId) => {\r\n      const category = categories.find(cat => cat.id === categoryId);\r\n      return category ? category.name : \"Unknown Category\";\r\n    };\r\n\r\n    const formatPrice = (price) => {\r\n      if (!price) return '';\r\n      return price.toLocaleString(\"ko-KR\") + \"원\";\r\n    };\r\n\r\n    const discountPrice = (price) => {\r\n      if (!price) return '';\r\n      return (price * 0.75).toLocaleString(\"ko-KR\") + \"원\";\r\n    };\r\n\r\n    onMounted(() => {\r\n      const imgSrc = `/api/items/${props.item.id}/image`;\r\n      itemImg.value = imgSrc;\r\n    });\r\n\r\n\r\n\r\n\r\n    return { goToProductDetail, getCategoryName, itemImg, formatPrice, discountPrice };\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n@import '../assets/css/components/similar_card.css'\r\n</style>"],"mappings":";AAsBA,OAAOA,MAAK,MAAO,kBAAkB;AACrC,SAASC,UAAS,QAAS,sBAAsB;AACjD,SAASC,GAAG,EAAEC,SAAQ,QAAS,KAAK;AAGpC,eAAe;EACbC,IAAI,EAAE,mBAAmB;EACzBC,KAAK,EAAE;IACLC,IAAI,EAAEC;EACR,CAAC;EACDC,KAAKA,CAACH,KAAK,EAAE;IACX,MAAMI,OAAM,GAAIP,GAAG,CAAC,IAAI,CAAC;IAEzB,MAAMQ,iBAAgB,GAAIA,CAAA,KAAM;MAE9BC,MAAM,CAACC,QAAQ,CAAC;QAAEC,GAAG,EAAE,CAAC;QAAEC,QAAQ,EAAE;MAAS,CAAC,CAAC;MAC/CH,MAAM,CAACI,gBAAgB,CAAC,QAAQ,EAAEC,oBAAoB,CAAC;IAGzD,CAAC;IAGD,MAAMA,oBAAmB,GAAIA,CAAA,KAAM;MACjC,IAAKL,MAAM,CAACM,WAAU,GAAIN,MAAM,CAACO,OAAO,IAAKC,QAAQ,CAACC,IAAI,CAACC,YAAY,EAAE;QACvE;QACAV,MAAM,CAACW,mBAAmB,CAAC,QAAQ,EAAEN,oBAAoB,CAAC;QAC1DhB,MAAM,CAACuB,IAAI,CAAC;UAAEnB,IAAI,EAAE,eAAe;UAAEoB,MAAM,EAAE;YAAEC,EAAE,EAAEpB,KAAK,CAACC,IAAI,CAACmB;UAAG;QAAE,CAAC,CAAC;QACrEd,MAAM,CAACe,QAAQ,CAACC,MAAM,CAAC,CAAC;MAC1B;IACF,CAAC;IAED,MAAMC,eAAc,GAAKC,UAAU,IAAK;MACtC,MAAMC,QAAO,GAAI7B,UAAU,CAAC8B,IAAI,CAACC,GAAE,IAAKA,GAAG,CAACP,EAAC,KAAMI,UAAU,CAAC;MAC9D,OAAOC,QAAO,GAAIA,QAAQ,CAAC1B,IAAG,GAAI,kBAAkB;IACtD,CAAC;IAED,MAAM6B,WAAU,GAAKC,KAAK,IAAK;MAC7B,IAAI,CAACA,KAAK,EAAE,OAAO,EAAE;MACrB,OAAOA,KAAK,CAACC,cAAc,CAAC,OAAO,IAAI,GAAG;IAC5C,CAAC;IAED,MAAMC,aAAY,GAAKF,KAAK,IAAK;MAC/B,IAAI,CAACA,KAAK,EAAE,OAAO,EAAE;MACrB,OAAO,CAACA,KAAI,GAAI,IAAI,EAAEC,cAAc,CAAC,OAAO,IAAI,GAAG;IACrD,CAAC;IAEDhC,SAAS,CAAC,MAAM;MACd,MAAMkC,MAAK,GAAK,cAAahC,K