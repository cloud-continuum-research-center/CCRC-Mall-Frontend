\\front-end\\src\\components\\SimilarCard.vue"],"sourcesContent":["<template>\r\n  <div class=\"similar-custom-card\" @click=\"goToProductDetail\">\r\n\r\n\r\n    <div class=\"image-container\">\r\n      <img :src=\"itemImg\" alt=\"Product Image\">\r\n    </div>\r\n\r\n    <div class=\"content-container\">\r\n      <div class=\"product-category-container\">\r\n        <h5 class=\"product-name\">{{ item.name }}</h5>\r\n        <p class=\"category\">카테고리 : {{ getCategoryName(item.category_id) }}</p>\r\n      </div>\r\n      <div class=\"price\">\r\n        <div class=\"discounted-price\">{{ discountPrice(item.price) }}</div>\r\n        <div class=\"original-price\">{{ formatPrice(item.price) }}</div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport router from \"@/scripts/router\";\r\nimport { categories } from \"@/scripts/categories\";\r\nimport { ref, onMounted } from \"vue\";\r\nimport axios from \"axios\";\r\n\r\n\r\nexport default {\r\n  name: \"SimilarCustomCard\",\r\n  props: {\r\n    item: Object\r\n  },\r\n  setup(props) {\r\n    const itemImg = ref(null);\r\n\r\n    const goToProductDetail = () => {\r\n\r\n      window.scrollTo({ top: 0, behavior: 'smooth' });\r\n      router.push({ name: 'ProductDetail', params: { id: props.item.id } });\r\n      setTimeout(() => {\r\n        window.location.reload();\r\n      }, 500);\r\n    };\r\n\r\n    const getCategoryName = (categoryId) => {\r\n      const category = categories.find(cat => cat.id === categoryId);\r\n      return category ? category.name : \"Unknown Category\";\r\n    };\r\n\r\n    const formatPrice = (price) => {\r\n      if (!price) return '';\r\n      return price.toLocaleString(\"ko-KR\") + \"원\";\r\n    };\r\n\r\n    const discountPrice = (price) => {\r\n      if (!price) return '';\r\n      return (price * 0.75).toLocaleString(\"ko-KR\") + \"원\";\r\n    };\r\n\r\n\r\n\r\n\r\n    onMounted(() => {\r\n      axios.get(`/api/items/${props.item.id}/multi/`).then(({ data }) => {\r\n        itemImg.value = data.image_path;\r\n      });\r\n      /*\r\n      const imgSrc = `/api/items/${props.item.id}/image`;\r\n      itemImg.value = imgSrc;*/\r\n    });\r\n\r\n\r\n\r\n\r\n    return { goToProductDetail, getCategoryName, itemImg, formatPrice, discountPrice };\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n@import '../assets/css/components/similar_card.css'\r\n</style>"],"mappings":";AAsBA,OAAOA,MAAK,MAAO,kBAAkB;AACrC,SAASC,UAAS,QAAS,sBAAsB;AACjD,SAASC,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,OAAOC,KAAI,MAAO,OAAO;AAGzB,eAAe;EACbC,IAAI,EAAE,mBAAmB;EACzBC,KAAK,EAAE;IACLC,IAAI,EAAEC;EACR,CAAC;EACDC,KAAKA,CAACH,KAAK,EAAE;IACX,MAAMI,OAAM,GAAIR,GAAG,CAAC,IAAI,CAAC;IAEzB,MAAMS,iBAAgB,GAAIA,CAAA,KAAM;MAE9BC,MAAM,CAACC,QAAQ,CAAC;QAAEC,GAAG,EAAE,CAAC;QAAEC,QAAQ,EAAE;MAAS,CAAC,CAAC;MAC/Cf,MAAM,CAACgB,IAAI,CAAC;QAAEX,IAAI,EAAE,eAAe;QAAEY,MAAM,EAAE;UAAEC,EAAE,EAAEZ,KAAK,CAACC,IAAI,CAACW;QAAG;MAAE,CAAC,CAAC;MACrEC,UAAU,CAAC,MAAM;QACfP,MAAM,CAACQ,QAAQ,CAACC,MAAM,CAAC,CAAC;MAC1B,CAAC,EAAE,GAAG,CAAC;IACT,CAAC;IAED,MAAMC,eAAc,GAAKC,UAAU,IAAK;MACtC,MAAMC,QAAO,GAAIvB,UAAU,CAACwB,IAAI,CAACC,GAAE,IAAKA,GAAG,CAACR,EAAC,KAAMK,UAAU,CAAC;MAC9D,OAAOC,QAAO,GAAIA,QAAQ,CAACnB,IAAG,GAAI,kBAAkB;IACtD,CAAC;IAED,MAAMsB,WAAU,GAAKC,KAAK,IAAK;MAC7B,IAAI,CAACA,KAAK,EAAE,OAAO,EAAE;MACrB,OAAOA,KAAK,CAACC,cAAc,CAAC,OAAO,IAAI,GAAG;IAC5C,CAAC;IAED,MAAMC,aAAY,GAAKF,KAAK,IAAK;MAC/B,IAAI,CAACA,KAAK,EAAE,OAAO,EAAE;MACrB,OAAO,CAACA,KAAI,GAAI,IAAI,EAAEC,cAAc,CAAC,OAAO,IAAI,GAAG;IACrD,CAAC;IAKD1B,SAAS,CAAC,MAAM;MACdC,KAAK,CAAC2B,GAAG,CAAE,cAAazB,KAAK,CAACC,IAAI,CAACW,EAAG,SAAQ,CAAC,CAACc,IAAI,CAAC,CAAC;QAAEC;MAAK,CAAC,KAAK;QACjEvB,OAAO,CAACwB,KAAI,GAAID,IAAI,CAACE,UAAU;MACjC,CAAC,CAAC;MACF;;;IAGF,CAAC,CAAC;IAKF,OAAO;MAAExB,iBAAiB;MAAEW,eAAe;MAAEZ,OAAO;MAAEiB,WAAW;MAAEG;IAAc,CAAC;EACpF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}