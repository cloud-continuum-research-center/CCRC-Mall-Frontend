{"ast":null,"code":"import axios from \"axios\";\nimport { reactive, onMounted } from \"vue\";\nimport { useRoute } from 'vue-router';\nimport Review from \"@/components/Review.vue\";\nimport * as SPLAT from \"https://cdn.jsdelivr.net/npm/gsplat@latest\";\nexport default {\n  name: \"ProductDetail\",\n  components: {\n    Review\n  },\n  setup() {\n    const route = useRoute();\n    const productId = route.params.id;\n    const state = reactive({\n      product: {}\n    });\n    const orderItem = () => {\n      window.alert('상품을 주문하였습니다!');\n    };\n    onMounted(() => {\n      axios.get(`/api/items/${productId}`).then(({\n        data\n      }) => {\n        state.product = data;\n      }).catch(error => {\n        console.error(\"Error fetching product:\", error);\n      });\n    });\n    onMounted(async () => {\n      const canvas = document.getElementById(\"canvas\");\n      const renderer = new SPLAT.WebGLRenderer(canvas);\n      const scene = new SPLAT.Scene();\n      const camera = new SPLAT.Camera();\n      const controls = new SPLAT.OrbitControls(camera, canvas);\n      const url = \"https://huggingface.co/datasets/dylanebert/3dgs/resolve/main/bonsai/bonsai-7k-mini.splat\";\n      await SPLAT.Loader.LoadAsync(url, scene);\n      const handleResize = () => {\n        renderer.setSize(window.innerWidth, window.innerHeight);\n      };\n      const frame = () => {\n        controls.update();\n        renderer.render(scene, camera);\n        requestAnimationFrame(frame);\n      };\n      handleResize();\n      window.addEventListener(\"resize\", handleResize);\n      requestAnimationFrame(frame);\n    });\n    return {\n      state,\n      orderItem\n    };\n  }\n};","map":{"version":3,"names":["axios","reactive","onMounted","useRoute","Review","SPLAT","name","components","setup","route","productId","params","id","state","product","orderItem","window","alert","get","then","data","catch","error","console","canvas","document","getElementById","renderer","WebGLRenderer","scene","Scene","camera","Camera","controls","OrbitControls","url","Loader","LoadAsync","handleResize","setSize","innerWidth","innerHeight","frame","update","render","requestAnimationFrame","addEventListener"],"sources":["C:\\Users\\boo\\icns\\shop\\front-end\\src\\pages\\ProductDetail.vue"],"sourcesContent":["<template>\r\n  <div class=\"product-detail\">\r\n    <!-- 상품 상세 정보 -->\r\n    <div v-if=\"state.product\">\r\n      <h2>{{ state.product.name }}</h2>\r\n      <p>Description: {{ state.product.description }}</p>\r\n      <p>Price: {{ state.product.price }}</p>\r\n    </div>\r\n    <div v-else>\r\n      <p>Loading...</p>\r\n    </div>\r\n    <button @click=\"orderItem\" class=\"btn btn-primary\">주문하기</button>\r\n  \r\n    <!-- 3D 모델을 표시할 캔버스 -->\r\n  <canvas ref=\"canvas\"></canvas>\r\n  \r\n  <Review :productId=\"state.product.id\"/>\r\n  </div>\r\n\r\n\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { reactive, onMounted } from \"vue\";\r\nimport { useRoute } from 'vue-router';\r\nimport Review from \"@/components/Review.vue\";\r\nimport * as SPLAT from \"https://cdn.jsdelivr.net/npm/gsplat@latest\";\r\n\r\n\r\nexport default {\r\n  name: \"ProductDetail\",\r\n  components: { Review },\r\n\r\n  setup() {\r\n    const route = useRoute();\r\n    const productId = route.params.id;\r\n\r\n    const state = reactive({\r\n      product: {},\r\n    });\r\n    \r\n    const orderItem = () => {\r\n      window.alert('상품을 주문하였습니다!');\r\n    };\r\n\r\n\r\n    onMounted(() => {\r\n      axios.get(`/api/items/${productId}`)\r\n        .then(({ data }) => {\r\n          state.product = data;\r\n        })\r\n        .catch(error => {\r\n          console.error(\"Error fetching product:\", error);\r\n        });\r\n    });\r\n\r\n    onMounted(async () => {\r\n      const canvas = document.getElementById(\"canvas\");\r\n      const renderer = new SPLAT.WebGLRenderer(canvas);\r\n      const scene = new SPLAT.Scene();\r\n      const camera = new SPLAT.Camera();\r\n      const controls = new SPLAT.OrbitControls(camera, canvas);\r\n\r\n      const url = \"https://huggingface.co/datasets/dylanebert/3dgs/resolve/main/bonsai/bonsai-7k-mini.splat\";\r\n      await SPLAT.Loader.LoadAsync(url, scene);\r\n\r\n      const handleResize = () => {\r\n        renderer.setSize(window.innerWidth, window.innerHeight);\r\n      };\r\n\r\n      const frame = () => {\r\n        controls.update();\r\n        renderer.render(scene, camera);\r\n        requestAnimationFrame(frame);\r\n      };\r\n\r\n      handleResize();\r\n      window.addEventListener(\"resize\", handleResize);\r\n      requestAnimationFrame(frame);\r\n    });\r\n\r\n    return { state, orderItem };\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAuBA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,QAAQ,EAAEC,SAAQ,QAAS,KAAK;AACzC,SAASC,QAAO,QAAS,YAAY;AACrC,OAAOC,MAAK,MAAO,yBAAyB;AAC5C,OAAO,KAAKC,KAAI,MAAO,4CAA4C;AAGnE,eAAe;EACbC,IAAI,EAAE,eAAe;EACrBC,UAAU,EAAE;IAAEH;EAAO,CAAC;EAEtBI,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIN,QAAQ,CAAC,CAAC;IACxB,MAAMO,SAAQ,GAAID,KAAK,CAACE,MAAM,CAACC,EAAE;IAEjC,MAAMC,KAAI,GAAIZ,QAAQ,CAAC;MACrBa,OAAO,EAAE,CAAC;IACZ,CAAC,CAAC;IAEF,MAAMC,SAAQ,GAAIA,CAAA,KAAM;MACtBC,MAAM,CAACC,KAAK,CAAC,cAAc,CAAC;IAC9B,CAAC;IAGDf,SAAS,CAAC,MAAM;MACdF,KAAK,CAACkB,GAAG,CAAE,cAAaR,SAAU,EAAC,EAChCS,IAAI,CAAC,CAAC;QAAEC;MAAK,CAAC,KAAK;QAClBP,KAAK,CAACC,OAAM,GAAIM,IAAI;MACtB,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAAC,yBAAyB,EAAEA,KAAK,CAAC;MACjD,CAAC,CAAC;IACN,CAAC,CAAC;IAEFpB,SAAS,CAAC,YAAY;MACpB,MAAMsB,MAAK,GAAIC,QAAQ,CAACC,cAAc,CAAC,QAAQ,CAAC;MAChD,MAAMC,QAAO,GAAI,IAAItB,KAAK,CAACuB,aAAa,CAACJ,MAAM,CAAC;MAChD,MAAMK,KAAI,GAAI,IAAIxB,KAAK,CAACyB,KAAK,CAAC,CAAC;MAC/B,MAAMC,MAAK,GAAI,IAAI1B,KAAK,CAAC2B,MAAM,CAAC,CAAC;MACjC,MAAMC,QAAO,GAAI,IAAI5B,KAAK,CAAC6B,aAAa,CAACH,MAAM,EAAEP,MAAM,CAAC;MAExD,MAAMW,GAAE,GAAI,0FAA0F;MACtG,MAAM9B,KAAK,CAAC+B,MAAM,CAACC,SAAS,CAACF,GAAG,EAAEN,KAAK,CAAC;MAExC,MAAMS,YAAW,GAAIA,CAAA,KAAM;QACzBX,QAAQ,CAACY,OAAO,CAACvB,MAAM,CAACwB,U