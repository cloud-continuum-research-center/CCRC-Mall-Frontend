{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport { reactive } from \"vue\";\nimport Card from \"@/components/Card.vue\";\nimport router from \"@/scripts/router\";\nexport default {\n  name: \"Home\",\n  components: {\n    Card\n  },\n  setup() {\n    const state = reactive({\n      items: [],\n      searchTerm: \"\",\n      searchBy: \"\"\n    });\n    const goToItemRegistration = () => {\n      router.push({\n        path: \"/new_item\"\n      });\n    };\n    axios.get(\"/api/items/\").then(({\n      data\n    }) => {\n      state.items = data;\n    });\n    const filterItems = () => {\n      // 검색어 없으면 모든 항목 표시\n      if (!searchTerm) return state.items;\n\n      // 검색 방식에 따라 필터링\n      if (searchBy === \"name\") {\n        return state.items.filter(item => item.name.includes(searchTerm));\n      } else if (searchBy === \"category\") {\n        return state.items.filter(item => item.category.includes(searchTerm));\n      }\n\n      // 검색 방식 선택 안했을 경우 모든 항목 표시\n      return state.items;\n    };\n    return {\n      state,\n      goToItemRegistration\n    };\n  },\n  computed: {\n    filteredItems() {\n      return filterItems();\n    }\n  }\n};","map":{"version":3,"names":["axios","reactive","Card","router","name","components","setup","state","items","searchTerm","searchBy","goToItemRegistration","push","path","get","then","data","filterItems","filter","item","includes","category","computed","filteredItems"],"sources":["C:\\Users\\boo\\icns\\shop\\front-end\\src\\pages\\Home.vue"],"sourcesContent":["<template>\r\n  <div class=\"home\">\r\n    <div class=\"album py-5 bg-body-tertiary\">\r\n      <div class=\"container\">\r\n        <button @click=\"goToItemRegistration\" class=\"btn btn-primary\">상품 등록</button>\r\n        <div></div>\r\n\r\n        <!-- 검색창 -->\r\n        <input type=\"text\" v-model=\"searchTerm\" placeholder=\"검색어를 입력하세요\" />\r\n        <label><input type=\"checkbox\" v-model=\"searchBy\" value=\"name\"> 상품명으로 검색</label>\r\n        <label><input type=\"checkbox\" v-model=\"searchBy\" value=\"category\"> 카테고리로 검색</label>\r\n        <button @click=\"filterItems\" class=\"btn btn-primary\">검색</button>\r\n\r\n\r\n\r\n        <div class=\"row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3\">\r\n          <div class=\"col\" v-for=\"(item, idx) in state.items\" :key=\"idx\">\r\n            <Card :item=\"item\" />\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { reactive } from \"vue\";\r\nimport Card from \"@/components/Card.vue\";\r\nimport router from \"@/scripts/router\";\r\n\r\nexport default {\r\n  name: \"Home\",\r\n  components: { Card },\r\n  setup() {\r\n    const state = reactive({\r\n      items: [],\r\n      searchTerm: \"\",\r\n      searchBy: \"\"\r\n    });\r\n\r\n\r\n    const goToItemRegistration = () => {\r\n      router.push({ path: \"/new_item\" });\r\n    };\r\n\r\n\r\n    axios.get(\"/api/items/\").then(({ data }) => {\r\n      state.items = data;\r\n    });\r\n\r\n    const filterItems = () => {\r\n      // 검색어 없으면 모든 항목 표시\r\n      if (!searchTerm) return state.items;\r\n\r\n      // 검색 방식에 따라 필터링\r\n      if (searchBy === \"name\") {\r\n        return state.items.filter(item => item.name.includes(searchTerm));\r\n      } else if (searchBy === \"category\") {\r\n        return state.items.filter(item => item.category.includes(searchTerm));\r\n      }\r\n\r\n      // 검색 방식 선택 안했을 경우 모든 항목 표시\r\n      return state.items;\r\n    };\r\n\r\n\r\n\r\n    return { state, goToItemRegistration };\r\n  },\r\n  \r\n  computed: {\r\n    filteredItems() {\r\n      return filterItems();\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped></style>"],"mappings":";AA4BA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,QAAO,QAAS,KAAK;AAC9B,OAAOC,IAAG,MAAO,uBAAuB;AACxC,OAAOC,MAAK,MAAO,kBAAkB;AAErC,eAAe;EACbC,IAAI,EAAE,MAAM;EACZC,UAAU,EAAE;IAAEH;EAAK,CAAC;EACpB