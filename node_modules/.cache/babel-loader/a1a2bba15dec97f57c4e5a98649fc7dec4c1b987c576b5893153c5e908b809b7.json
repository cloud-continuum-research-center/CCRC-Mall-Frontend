{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport router from \"@/scripts/router\";\nimport axios from \"axios\";\nimport { categories } from \"@/scripts/categories\";\nimport { reactive } from \"vue\";\nexport default {\n  name: \"CustomCard\",\n  props: {\n    item: Object\n  },\n  setup(props) {\n    const state = reactive({\n      itemImg: null\n    });\n    const goToProductDetail = () => {\n      router.push({\n        name: 'ProductDetail',\n        params: {\n          id: props.item.id\n        }\n      });\n    };\n    const getCategoryName = categoryId => {\n      const category = categories.find(cat => cat.id === categoryId);\n      return category ? category.name : \"Unknown Category\";\n    };\n    axios.get(`/api/items/${props.item.id}/image`).then(response => {\n      state.itemImg = response.data;\n    }).catch(error => {\n      console.error('Error fetching image:', error);\n    });\n    return {\n      goToProductDetail,\n      getCategoryName,\n      state\n    };\n  }\n};","map":{"version":3,"names":["router","axios","categories","reactive","name","props","item","Object","setup","state","itemImg","goToProductDetail","push","params","id","getCategoryName","categoryId","category","find","cat","get","then","response","data","catch","error","console"],"sources":["C:\\Users\\boo\\icns\\shop\\front-end\\src\\components\\Card.vue"],"sourcesContent":["<template>\r\n  <div class=\"custom-card\" @click=\"goToProductDetail\">\r\n\r\n\r\n    <div class=\"image-container\">\r\n      <img :src=\"state.itemImg\" alt=\"Product Image\">\r\n      <div class=\"discount-badge\">\r\n        -30%\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"content-container\">\r\n      <div class=\"product-category-container\">\r\n        <h5 class=\"product-name\">{{ item.name }}</h5>\r\n        <p class=\"category\">{{ getCategoryName(item.category_id) }}</p>\r\n      </div>\r\n      <div class=\"price\">\r\n        <div class=\"discounted-price\">Rp 3.500.000</div>\r\n        <div class=\"original-price\">Rp 2.500.000</div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport router from \"@/scripts/router\";\r\nimport axios from \"axios\";\r\nimport { categories } from \"@/scripts/categories\";\r\nimport { reactive } from \"vue\";\r\n\r\n\r\nexport default {\r\n  name: \"CustomCard\",\r\n  props: {\r\n    item: Object\r\n  },\r\n  setup(props) {\r\n    const state = reactive({\r\n      itemImg : null,\r\n  });\r\n\r\n    const goToProductDetail = () => {\r\n      router.push({ name: 'ProductDetail', params: { id: props.item.id } });\r\n    };\r\n    const getCategoryName = (categoryId) => {\r\n      const category = categories.find(cat => cat.id === categoryId);\r\n      return category ? category.name : \"Unknown Category\";\r\n    };\r\n\r\n    axios.get(`/api/items/${props.item.id}/image`)\r\n      .then(response => {\r\n        state.itemImg = response.data;\r\n      })\r\n      .catch(error => {\r\n        console.error('Error fetching image:', error);\r\n      });\r\n\r\n    return { goToProductDetail, getCategoryName, state };\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.custom-card {\r\n  width: 285px;\r\n  height: 446px;\r\n  position: relative;\r\n}\r\n\r\n.image-container {\r\n  width: 100%;\r\n  height: 301px;\r\n  position: absolute;\r\n}\r\n\r\n.image-container img {\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n\r\n.content-container {\r\n  width: 100%;\r\n  height: 145px;\r\n  position: absolute;\r\n  bottom: 0;\r\n  background: #F4F5F7;\r\n  padding: 16px;\r\n}\r\n\r\n.product-category-container {\r\n  width: 125px;\r\n  height: 61px;\r\n  position: relative;\r\n}\r\n\r\n.product-name {\r\n  color: #3A3A3A;\r\n  font-size: 24px;\r\n  font-family: Gilroy;\r\n  font-weight: 600;\r\n  line-height: 28.80px;\r\n  word-wrap: break-word;\r\n}\r\n\r\n.category {\r\n  color: #898989;\r\n  font-size: 16px;\r\n  font-family: Gilroy;\r\n  font-weight: 500;\r\n  line-height: 24px;\r\n  word-wrap: break-word;\r\n}\r\n\r\n\r\n.price {\r\n  width: 238px;\r\n  height: 30px;\r\n  justify-content: flex-start;\r\n  align-items: center;\r\n  gap: 16px;\r\n  display: inline-flex;\r\n}\r\n\r\n.original-price {\r\n  color: #B0B0B0;\r\n  font-size: 16px;\r\n  font-family: Gilroy;\r\n  font-weight: 400;\r\n  text-decoration: line-through;\r\n  line-height: 24px;\r\n  word-wrap: break-word;\r\n}\r\n\r\n.discounted-price {\r\n  color: #3A3A3A;\r\n  font-size: 20px;\r\n  font-family: Gilroy;\r\n  font-weight: 600;\r\n  line-height: 30px;\r\n  word-wrap: break-word;\r\n}\r\n\r\n.discount-badge {\r\n  width: 48px;\r\n  height: 48px;\r\n  position: absolute;\r\n  top: 24px;\r\n  right: 0;\r\n  background: #E97171;\r\n  border-radius: 50%;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  color: white;\r\n  font-size: 16px;\r\n  font-weight: 500;\r\n}\r\n</style>\r\n"],"mappings":";AAyBA,OAAOA,MAAK,MAAO,kBAAkB;AACrC,OAAOC,KAAI,MAAO,OAAO;AACzB,SAASC,UAAS,QAAS,sBAAsB;AACjD,SAASC,QAAO,QAAS,KAAK;AAG9B,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,KAAK,EAAE;IACLC,IAAI,EAAEC;EACR,CAAC;EACDC,KAAKA,CAACH,KAAK,EAAE;IACX,MAAMI,KAAI,GAAIN,QAAQ,CAAC;MACrBO,OAAM,EAAI;IACd,CAAC,CAAC;IAEA,MAAMC,iBAAgB,GAAIA,CAAA,KAAM;MAC9BX,MAAM,CAACY,IAAI,CAAC;QAAER,IAAI,EAAE,eAAe;QAAES,MAAM,EAAE;UAAEC,EAAE,EAAET,KAAK,CAACC,IAAI,CAACQ;QAAG;MAAE,CAAC,CAAC;IACvE,CAAC;IACD,MAAMC,eAAc,GAAKC,UAAU,IAAK;MACtC,MAAMC,QAAO,GAAIf,UAAU,CAACgB,IAAI,CAACC,GAAE,IAAKA,GAAG,CAACL,EAAC,KAAME,UAAU,CAAC;MAC9D,OAAOC,QAAO,GAAIA,QAAQ,CAACb,IAAG,GAAI,kBAAkB;IACtD,CAAC;IAEDH,KAAK,CAACmB,GAAG,CAAE,cAAaf,KAAK,CAACC,IAAI,CAACQ,EAAG,QAAO,EAC1CO,IAAI,CAACC,QAAO,IAAK;MAChBb,KAAK,CAACC,OAAM,GAAIY,QAAQ,CAACC,IAAI;IAC/B,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;MACdC,OAAO,CAACD,KAAK,CAAC,uBAAuB,EAAEA,KAAK,CAAC;IAC/C,CAAC,CAAC;IAEJ,OAAO;MAAEd,iBAAiB;MAAEI,eAAe;MAAEN;IAAM,CAAC;EACtD;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}