{"ast":null,"code":"import axios from \"axios\";\nimport { reactive, onMounted } from \"vue\";\nimport { useRoute } from 'vue-router';\nexport default {\n  name: \"ProductDetail\",\n  setup() {\n    const route = useRoute();\n    const productId = route.params.id;\n    const state = reactive({\n      product: {},\n      reviews: [],\n      newReview: {\n        content: \"\",\n        star: 1\n      }\n    });\n    onMounted(() => {\n      axios.get(`/api/items/${productId}`).then(({\n        data\n      }) => {\n        state.product = data;\n        console.log(state.product);\n      }).catch(error => {\n        console.error(\"상품을 불러오는 동안 오류 발생:\", error);\n      });\n    });\n    const submitReview = () => {\n      axios.post(`/api/items/${productId}/reviews/`, state.newReview).then(() => {\n        state.newReview.content = \"\";\n        state.newReview.star = 1;\n      }).catch(error => {\n        console.error(\"Error submitting review:\", error);\n      });\n    };\n    return {\n      state,\n      submitReview\n    };\n  }\n};","map":{"version":3,"names":["axios","reactive","onMounted","useRoute","name","setup","route","productId","params","id","state","product","reviews","newReview","content","star","get","then","data","console","log","catch","error","submitReview","post"],"sources":["C:\\Users\\boo\\icns\\shop\\front-end\\src\\pages\\ProductDetail.vue"],"sourcesContent":["<template>\r\n  <div class=\"product-detail\">\r\n\r\n    <!-- 상품 상세 정보 -->\r\n    <div v-if=\"state.product\">\r\n      <h2>{{ state.product.name }}</h2>\r\n <!-- <img :src=\"state.product.image\" :alt=\"state.product.name\" />-->\r\n      <p>Description: {{ state.product.description }}</p>\r\n      <p>Price: {{ state.product.price }}</p>\r\n    </div>\r\n    <div v-else>\r\n      <p>Loading...</p>\r\n    </div>\r\n\r\n    \r\n    <!-- 리뷰 작성-->\r\n    <form @submit.prevent=\"submitReview\">\r\n      <label for=\"review\">Your Review:</label><br>\r\n      <textarea id=\"review\" v-model=\"newReview.content\" required></textarea><br>\r\n      <label for=\"rating\">Rating:</label>\r\n      <input type=\"number\" id=\"rating\" v-model=\"newReview.star\" min=\"1\" max=\"5\" required><br>\r\n      <button type=\"submit\">Submit Review</button>\r\n    </form>\r\n\r\n\r\n</div>\r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\nimport { reactive, onMounted } from \"vue\";\r\nimport { useRoute } from 'vue-router';\r\n\r\nexport default {\r\n  name: \"ProductDetail\",\r\n\r\n  setup() {\r\n    const route = useRoute();\r\n    const productId = route.params.id;\r\n\r\n    const state = reactive({\r\n      product: {},\r\n      reviews: [],\r\n      newReview: { content: \"\", star: 1 }\r\n\r\n    });\r\n\r\n\r\n\r\n\r\n    onMounted(() => {\r\n      axios.get(`/api/items/${productId}`).then(({ data }) => {\r\n        state.product = data;\r\n        console.log(state.product);\r\n      }).catch(error => {\r\n        console.error(\"상품을 불러오는 동안 오류 발생:\", error);       \r\n      });\r\n\r\n    });\r\n\r\n   const submitReview = () => {\r\n      axios.post(`/api/items/${productId}/reviews/`, state.newReview)\r\n        .then(() => {\r\n          state.newReview.content = \"\";\r\n          state.newReview.star = 1;\r\n\r\n        })\r\n        .catch(error => {\r\n          console.error(\"Error submitting review:\", error);\r\n        });\r\n    };\r\n\r\n\r\n    return { state,submitReview };\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AA4BA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,QAAQ,EAAEC,SAAQ,QAAS,KAAK;AACzC,SAASC,QAAO,QAAS,YAAY;AAErC,eAAe;EACbC,IAAI,EAAE,eAAe;EAErBC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIH,QAAQ,CAAC,CAAC;IACxB,MAAMI,SAAQ,GAAID,KAAK,CAACE,MAAM,CAACC,EAAE;IAEjC,MAAMC,KAAI,GAAIT,QAAQ,CAAC;MACrBU,OAAO,EAAE,CAAC,CAAC;MACXC,OAAO,EAAE,EAAE;MACXC,SAAS,EAAE;QAAEC,OAAO,EAAE,EAAE;QAAEC,IAAI,EAAE;MAAE;IAEpC,CAAC,CAAC;IAKFb,SAAS,CAAC,MAAM;MACdF,KAAK,CAACgB,GAAG,CAAE,cAAaT,SAAU,EAAC,CAAC,CAACU,IAAI,CAAC,CAAC;QAAEC;MAAK,CAAC,KAAK;QACtDR,KAAK,CAACC,