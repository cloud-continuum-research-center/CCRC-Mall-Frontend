{"ast":null,"code":"import axios from \"axios\";\nimport { reactive, onMounted, ref } from \"vue\";\nimport { useRoute } from 'vue-router';\nimport Review from \"@/components/Review.vue\";\nimport CanvasRenderer from \"@/components/CanvasRenderer.vue\";\nexport default {\n  name: \"ProductDetail\",\n  components: {\n    Review,\n    CanvasRenderer\n  },\n  setup() {\n    const route = useRoute();\n    const productId = route.params.id;\n    const state = reactive({\n      product: {}\n    });\n    const isPopupVisible = ref(false);\n    const orderItem = () => {\n      window.alert('상품을 주문하였습니다!');\n    };\n    const openPopup = () => {\n      isPopupVisible.value = true;\n    };\n    const closePopup = () => {\n      isPopupVisible.value = false;\n    };\n\n    //const modelUrl = \"https://huggingface.co/datasets/dylanebert/3dgs/resolve/main/bonsai/bonsai-7k.splat\";\n    const modelUrl = \"/point_cloud.ply\";\n    onMounted(() => {\n      axios.get(`/api/items/${productId}`).then(({\n        data\n      }) => {\n        state.product = data;\n      }).catch(error => {\n        console.error(\"Error fetching product:\", error);\n      });\n    });\n    return {\n      state,\n      orderItem,\n      modelUrl,\n      openPopup,\n      closePopup,\n      isPopupVisible\n    };\n  }\n};","map":{"version":3,"names":["axios","reactive","onMounted","ref","useRoute","Review","CanvasRenderer","name","components","setup","route","productId","params","id","state","product","isPopupVisible","orderItem","window","alert","openPopup","value","closePopup","modelUrl","get","then","data","catch","error","console"],"sources":["C:\\Users\\boo\\icns\\shop\\front-end\\src\\pages\\ProductDetail.vue"],"sourcesContent":["<template>\r\n  <div class=\"product-detail\">\r\n    <!-- 상품 상세 정보 -->\r\n    <div v-if=\"state.product\">\r\n      <h2>{{ state.product.name }}</h2>\r\n      <p class=\"description\">Description: {{ state.product.description }}</p>\r\n      <p class=\"price\">Price: {{ state.product.price }}</p>\r\n    </div>\r\n    <div v-else>\r\n      <p>Loading...</p>\r\n    </div>\r\n    <button @click=\"orderItem\" class=\"btn btn-primary\">주문하기</button>\r\n    <button @click=\"openPopup\" class=\"btn btn-secondary\">3D로 보기</button>\r\n\r\n\r\n    <div v-if=\"isPopupVisible\" class=\"popup-overlay\">\r\n      <div class=\"popup-content\">\r\n        <button @click=\"closePopup\" class=\"close-btn\">X</button>\r\n        <Rendering :url=\"modelUrl\" />\r\n      </div>\r\n    </div>\r\n\r\n    <Review :productId=\"state.product.id\" />\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { reactive, onMounted, ref } from \"vue\";\r\nimport { useRoute } from 'vue-router';\r\nimport Review from \"@/components/Review.vue\";\r\nimport CanvasRenderer from \"@/components/CanvasRenderer.vue\";\r\n\r\n\r\nexport default {\r\n  name: \"ProductDetail\",\r\n  components: { Review, CanvasRenderer },\r\n\r\n  setup() {\r\n    const route = useRoute();\r\n    const productId = route.params.id;\r\n\r\n    const state = reactive({\r\n      product: {},\r\n    });\r\n\r\n    const isPopupVisible = ref(false);\r\n\r\n    const orderItem = () => {\r\n      window.alert('상품을 주문하였습니다!');\r\n    };\r\n\r\n    const openPopup = () => {\r\n      isPopupVisible.value = true;\r\n    };\r\n\r\n    const closePopup = () => {\r\n      isPopupVisible.value = false;\r\n    };\r\n\r\n\r\n    //const modelUrl = \"https://huggingface.co/datasets/dylanebert/3dgs/resolve/main/bonsai/bonsai-7k.splat\";\r\n    const modelUrl=\"/point_cloud.ply\";\r\n\r\n    onMounted(() => {\r\n      axios.get(`/api/items/${productId}`)\r\n        .then(({ data }) => {\r\n          state.product = data;\r\n        })\r\n        .catch(error => {\r\n          console.error(\"Error fetching product:\", error);\r\n        });\r\n    });\r\n\r\n    return { state, orderItem, modelUrl, openPopup, closePopup, isPopupVisible };\r\n  },\r\n};\r\n</script>\r\n\r\n<style 